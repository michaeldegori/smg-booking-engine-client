 });\n\n    return api\n      .post('http://localhost:3000/properties', formData)\n      .then((res) => {\n        history.push('/properties');\n        console.log('Property Uploaded Successfully');\n      })\n      .catch((err) => {\n        alert('Upload Error');\n      });\n  };\n\n  return (\n    <section className=\"add-property\">\n      <div className=\"container-xl \">\n        <div className=\"row d-flex justify-content-center\">\n          <div className=\"col-md-8\">\n            <h5 className=\"card-title py-2\">Add new property</h5>\n            <form onSubmit={submit}>\n              <div className=\"form-group\">\n                <div className=\"form-group\">\n                  <small>Photos</small>\n                  <input\n                    type=\"file\"\n                    name=\"photos\"\n                    onChange={handlePhotoFiles}\n                  />\n                </div>\n                <small>Bedrooms</small>\n                <input\n                  className=\"form-control py-4\"\n                  type=\"number\"\n                  name=\"bedrooms\"\n                  placeholder=\"# of bedrooms\"\n                  value={propertyDetails.bedrooms}\n                  onChange={handleChange}\n                />\n                <small>Bathrooms</small>\n                <input\n                  className=\"form-control py-4\"\n                  type=\"number\"\n                  name=\"bathrooms\"\n                  placeholder=\"# of bathrooms\"\n                  value={propertyDetails.bathrooms}\n                  onChange={handleChange}\n                />\n                <small>Title</small>\n                <input\n                  className=\"form-control py-4\"\n                  name=\"listingTitle\"\n                  placeholder=\"listing title\"\n                  value={propertyDetails.listingTitle}\n                  onChange={handleChange}\n                />\n                <small>Description</small>\n                <textarea\n                  className=\"form-control py-4\"\n                  name=\"description\"\n                  placeholder=\"description\"\n                  value={propertyDetails.description}\n                  onChange={handleChange}\n                />\n                <small>Address</small>\n                <input\n                  className=\"form-control py-4\"\n                  name=\"address\"\n                  placeholder=\"address\"\n                  value={propertyDetails.address}\n                  onChange={handleChange}\n                />\n                <small>Max guests</small>\n                <input\n                  className=\"form-control py-4\"\n                  type=\"number\"\n                  name=\"maxGuests\"\n                  placeholder=\"max # of guests\"\n                  value={propertyDetails.maxGuests}\n                  onChange={handleChange}\n                />\n                <small>Directions</small>\n                <textarea\n                  className=\"form-control py-4\"\n                  name=\"directions\"\n                  placeholder=\"directions\"\n                  value={propertyDetails.directions}\n                  onChange={handleChange}\n                />\n                <small>Check in instructions</small>\n                <textarea\n                  className=\"form-control py-4\"\n                  name=\"checkinInstructions\"\n                  placeholder=\"checkin instructions\"\n                  value={propertyDetails.checkinInstructions}\n                  onChange={handleChange}\n                />\n                <small>Wifi details</small>\n                <textarea\n                  className=\"form-control py-4\"\n                  name=\"wifiDetails\"\n                  placeholder=\"wifi info\"\n                  value={propertyDetails.wifiDetails}\n                  onChange={handleChange}\n                />\n                <small>House manual</small>\n                <input\n                  className=\"form-control py-4\"\n                  name=\"houseManual\"\n                  placeholder=\"house manual link\"\n                  value={propertyDetails.houseManual}\n                  onChange={handleChange}\n                />\n                <small>House rules</small>\n                <textarea\n                  className=\"form-control py-4\"\n                  name=\"houseRules\"\n                  placeholder=\"house rules\"\n                  value={propertyDetails.houseRules}\n                  onChange={handleChange}\n                />\n                <small>Cancellation Policy</small>\n                <textarea\n                  className=\"form-control py-4\"\n                  name=\"cancellationPolicy\"\n                  placeholder=\"cancellation policy\"\n                  value={propertyDetails.cancellationPolicy}\n                  onChange={handleChange}\n                />\n              </div>\n              <button className=\"form-control btn btn-primary text-white \">\n                Submit property\n              </button>\n            </form>\n            <hr />\n          </div>\n        </div>\n      </div>\n    </section>\n  );\n};\n\nexport default AddProperty;\n","/Users/michaeldegori/Desktop/Web Development/smg-direct-booking-site/client/src/components/PrivateRoute.js",[],"/Users/michaeldegori/Desktop/Web Development/smg-direct-booking-site/client/src/contexts/User.jsx",["184","185"],"import { createContext, useState, useEffect } from 'react';\nexport const userContext = createContext({\n  user: null,\n  setUser: () => {},\n});\nexport default function ({ children }) {\n  const [user, setUser] = useState();\n  return (\n    <userContext.Provider value={{ user, setUser }}>\n      {children}\n    </userContext.Provider>\n  );\n}\n","/Users/michaeldegori/Desktop/Web Development/smg-direct-booking-site/client/src/components/Signup.js",["186","187"],"/Users/michaeldegori/Desktop/