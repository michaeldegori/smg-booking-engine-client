n\nconst Booking = (props) => {\n  const history = useHistory();\n  const [property, setProperty] = useState();\n  const [user, setUser] = useState();\n\n  const [booking, setBooking] = useState({\n    checkinDate: '',\n    checkoutDate: '',\n  });\n\n  useEffect(() => {\n    api\n      .get(\n        `${process.env.REACT_APP_BACKEND}properties/${props.match.params.id}`\n      )\n      .then((res) => {\n        setProperty(res.data);\n      });\n  }, []);\n\n  const handleChange = (event) => {\n    setBooking({\n      ...booking,\n      [event.target.name]: event.target.value,\n    });\n  };\n\n  const submitBooking = (event) => {\n    event.preventDefault();\n\n    return api\n      .post(\n        `${process.env.REACT_APP_BACKEND}properties/${property._id}/reserve`,\n        {\n          property: props.match.params.id,\n          ...booking,\n          type: 'Direct',\n        }\n      )\n      .then((res) => {\n        history.push('/confirm-details');\n        console.log('Property Uploaded Successfully');\n      })\n      .catch((err) => {\n        alert('Upload Error');\n      });\n  };\n\n  return (\n    <section className=\"booking\">\n      <div className=\"container-xl \">\n        <div className=\"row d-flex justify-content-center\">\n          <div className=\"col-md-8\">\n            <h5 className=\"card-title py-2\">Book your getaway</h5>\n            <form onSubmit={submitBooking}>\n              <div className=\"form-group\">\n                <small>Check in Date</small>\n                <input\n                  className=\"form-control py-4\"\n                  type=\"date\"\n                  name=\"checkinDate\"\n                  value={booking.checkinDate}\n                  onChange={handleChange}\n                />\n                <small>Check out Date</small>\n                <input\n                  className=\"form-control py-4\"\n                  type=\"date\"\n                  name=\"checkoutDate\"\n                  value={booking.checkoutDate}\n                  onChange={handleChange}\n                />\n              </div>\n              <button className=\"form-control btn btn-primary text-white \">\n                Book now\n              </button>\n            </form>\n            <hr />\n          </div>\n        </div>\n      </div>\n    </section>\n  );\n};\n\nexport default Booking;\n","/Users/michaeldegori/Desktop/Web Development/smg-direct-booking-engine/client/src/components/BookingConfirmed.js",[],"/Users/michaeldegori/Desktop/Web Development/smg-direct-booking-engine/client/src/components/ConfirmBookingDetails.js",[],"/Users/michaeldegori/Desktop/Web Development/smg-direct-booking-engine/client/src/components/WalkerAirbnbCal.js",[],"/Users/michaeldegori/Desktop/Web Development/smg-direct-booking-engine/client/src/components/EasyAirbnbCal.js",[],"/Users/michaeldegori/Desktop/Web Development/smg-direct-booking-engine/client/src/components/WalkerVrboCal.js",[],"/Users/michaeldegori/Desktop/Web Development/smg-direct-booking-engine/client/src/components/EasyVrboCal.js",[],"/Users/michaeldegori/Desktop/Web Development/smg-direct-booking-engine/client/src/components/ICalImport.js",["208"],"import React from 'react';\nimport api from '../services/api';\nimport { useHistory } from 'react-router-dom';\nimport '../styles/EditProperty.css';\nimport ICAL from 'ical.js';\nimport mongoose from 'mongoose';\n\nconst ICalImport = (propertyId, source) => {\n  const history = useHistory();\n\n  const readFile = (event) => {\n    const file = event.target.files[0];\n    if (!file) {\n      return;\n    }\n    const reader = new FileReader();\n    reader.onload = function (event) {\n      let contents = event.target.result;\n      let parsedContents = ICAL.parse(contents);\n      let bookingData = parsedContents[2].map((eachEvent) =>\n        eachEvent[1].map((item) => item)\n      );\n      let bookingDates = bookingData.map((items) => {\n        let checkinDetail = items[1];\n        let checkoutDetail = items[0];\n        return {\n          checkinDate: new Date(checkinDetail[3]),\n          checkoutDate: new Date(checkoutDetail[3]),\n        };\n      });\n\n      bookingDates.map((eachBooking) => {\n        api\n          .post(\n            `${process.env.REACT_APP_BACKEND}properties/${propertyId}/reserve`,\n            {\n              property: mongoose.Types.ObjectId(`${propertyId}`),\n              ...eachBooking,\n              bookingSource: `${source}`,\n            }\n          )\n          .then((res) => {\n            history.push('/');\n            console.log('Calendar Uploaded Successfully');\n          })\n          .catch((err) => {\n            alert('Upload Error');\n          });\n      });\n    };\n    reader.readAsText(file);\n  };\n\n  // const values = iCal.map(function (date) {\n  //   return date.getTime();\n  // });\n  // const unique = availableDates.filter(function (date) {\n  //   return values.indexof(date.getTime()) == -1;\n  // });\n\n  return (\n    <div>\n      <input type=\"file\" onChange={readFile} />\n    </div>\n  );\n};\n\nexport default ICalImport;\n","/Users/michaeldegori/Desktop/Web Development/smg-direct-booking-engine/client/src/components/EasyDateRange.js",["209"],"import React, { useState, useEffect } from 'react';\nimport '../styles/PropertyDetails.css';\nimport Axios from 'axios';\nimport { DateRangePicker } from 'react-dates';\nimport '../styles/EasyDateRange.css';\nimport moment from 'moment';\nmoment().format();\n\nconst EasyDateRange = (props) => {\n  const [open, setOpen] = useState(false);\n  const [bookings, setBookings] = useState();\n  const [dateRange, setdateRange] = useState({\n    startDate: '',\n    endDate: '',\n  });\n\n  useEffect(() => {\n    Axios.get(\n      `${process.env.REACT_APP_BACKEND}get-bookings/5fd8cdc6ed2ea14c3a06f712`\n    )\n      .then((res) => {\n        setBookings(res.data);\n      })\n      .catch((err) => console.log(err));\n  }, []);\n\n  const [focus, setFocus] =